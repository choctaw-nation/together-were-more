{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "cno/twm-profile-swiper-block",
	"version": "1.0.0",
	"title": "Profile Swiper Block",
	"category": "theme",
	"icon": "block-default",
	"keywords": [ "swiper", "carousel", "slider" ],
	"description": "A php block that renders the Profile Swiper",
	"supports": {
		"html": false,
		"reusable": false,
		"multiple": false
	},
	"attributes": {
		"paginationColor": {
			"type": "string",
			"default": "gray"
		}
	},
	"editorScript": "file:./index.tsx",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js",
	"viewStyle": "file:./view.css"
}
