{
	"name": "together-were-more",
	"version": "1.1.8",
	"description": "Custom WP theme for Together We're More site",
	"browserslist": [
		"extends @wordpress/browserslist-config"
	],
	"prettier": "@wordpress/prettier-config",
	"scripts": {
		"build": "wp-scripts build --blocks-manifest --webpack-copy-php --source-path=wp-content/themes/together-were-more/src/ --output-path=wp-content/themes/together-were-more/dist/",
		"check-engines": "wp-scripts check-engines",
		"lint:css": "npx stylelint '**/*.scss' '**/*.css' --fix",
		"lint:js": "npx eslint --fix",
		"packages-update": "wp-scripts packages-update",
		"start": "wp-scripts start --blocks-manifest --webpack-copy-php --source-path=wp-content/themes/together-were-more/src/ --output-path=wp-content/themes/together-were-more/dist/",
		"test:dev": "echo 'Test not implemented yet' && exit 0",
		"test:stg": "echo 'Test not implemented yet' && exit 0"
	},
	"author": "Choctaw Nation of Oklahoma",
	"license": "GPL-3.0-or-later",
	"dependencies": {
		"@choctawnationofoklahoma/lite-vimeo": "^1.3.1",
		"@choctawnationofoklahoma/wp-flex-reverse-toggle": "^1.1.1",
		"aos": "^2.3.4",
		"bootstrap": "^5.3.7",
		"swiper": "^11.2.10"
	},
	"devDependencies": {
		"@eslint/compat": "^1.3.2",
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.34.0",
		"@wordpress/block-editor": "^15.2.0",
		"@wordpress/blocks": "^15.2.0",
		"@wordpress/components": "^30.2.0",
		"@wordpress/compose": "^7.29.0",
		"@wordpress/data": "^10.29.0",
		"@wordpress/eslint-plugin": "^22.15.0",
		"@wordpress/prettier-config": "^4.29.0",
		"@wordpress/scripts": "^30.22.0",
		"eslint": "^9.34.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-import-resolver-typescript": "^4.4.4",
		"eslint-plugin-react-hooks": "^5.2.0",
		"globals": "^16.3.0",
		"stylelint": "^16.23.1",
		"stylelint-config-standard-scss": "^15.0.1",
		"stylelint-scss": "^6.12.1",
		"webpack-remove-empty-scripts": "^1.1.1"
	}
}
