{
	"name": "together-were-more",
	"version": "1.1.4",
	"description": "Custom WP theme for Together We're More site",
	"prettier": "@wordpress/prettier-config",
	"browserslist": [
		"extends @wordpress/browserslist-config"
	],
	"scripts": {
		"build": "wp-scripts build --blocks-manifest --webpack-copy-php --source-path=wp-content/themes/together-were-more/src/ --output-path=wp-content/themes/together-were-more/dist/",
		"check-engines": "wp-scripts check-engines",
		"packages-update": "wp-scripts packages-update",
		"start": "wp-scripts start --blocks-manifest --webpack-copy-php --source-path=wp-content/themes/together-were-more/src/ --output-path=wp-content/themes/together-were-more/dist/",
		"test:dev": "echo 'Test not implemented yet' && exit 0",
		"test:stg": "echo 'Test not implemented yet' && exit 0"
	},
	"author": "Choctaw Nation of Oklahoma",
	"license": "GPL-3.0-or-later",
	"dependencies": {
		"@choctawnationofoklahoma/lite-vimeo": "^1.2.2",
		"aos": "^2.3.4",
		"bootstrap": "^5.3.6",
		"swiper": "^11.2.8"
	},
	"devDependencies": {
		"@wordpress/scripts": "^30.17.0",
		"webpack-remove-empty-scripts": "^1.1.1"
	}
}
